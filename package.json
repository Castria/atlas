{
  "name": "atlas",
  "private": true,
  "scripts": {
    "start": "hugo server",
    "build": "cross-env NODE_ENV=production hugo && npm run build:functions",
    "build:functions": "cross-env NODE_ENV=production netlify-lambda build assets/lambda",
    "server": "cross-env NODE_ENV=development hugo server --gc --disableFastRender --navigateToChanged"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "autoprefixer": "^9.6.1",
    "cross-env": "^5.2.0",
    "netlify-lambda": "^1.5.0",
    "postcss-cli": "^6.1.3"
  }
}
